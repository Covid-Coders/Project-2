{
  "env": {
      "browser": true,
      "commonjs": true,
      "node": true,
      "es6": true
  },
  "rules": {
      "no-duplicate-case":"error",
      "no-empty":"error",
      "no-extra-semi":"error",
      "no-func-assign": "error",
      "no-irregular-whitespace":"error",
      "no-unreachable": "error",
      "curly": "error",
      "dot-notation": "off",
      "eqeqeq": "error",
      "no-empty-function": "off",
      "no-multi-spaces": "error",
      "no-mixed-spaces-and-tabs": "error",
      "no-trailing-spaces": "off",
      "default-case": "error",
      "no-fallthrough": "error",
      "no-unused-vars": "off",
      "no-use-before-define": "error",
      "no-redeclare": "error",
      "camelcase": "error",
      "brace-style": "error",
      "indent": [
          "off",
          2
      ],
      "quotes": [
          "warn",
          "single"
      ],
      "semi": [
          "error",
          "always"
      ]
  }
}
